using System;
using System.Collections.Generic;
using System.Text;
using NoCap_3_5_6_8_;
using alex;

namespace alex
{
    class dialog
    {
        public dialog()
        {
            this.initPashukAlexOpenPashukCloseAlex();
            this.initPashukAlexCloseAlexAndPashuk();
            this.initPashukAlexAlexOpenBrov();
            this.paintPashukAlexAlexOpenBrovAlexClose();
        }

        public void questDialog(string str)
        {
            string output = "";
            int i = 0;
            while (output.Length != str.Length)
            {
                System.Threading.Thread.Sleep(70);
                    this.paintPashukAlexCloseAlexAndPashuk();
                Console.SetCursorPosition(0, 0);
                if (i % 6 == 0) //моргнул
                {
                    this.paintPashukAlexAlexOpenBrov();
                    Console.SetCursorPosition(0, 0);
                    System.Threading.Thread.Sleep(70);
                }
                if (i % 8 == 0) //моргнул
                {
                    this.paintPashukAlexOpenPashukCloseAlex();
                    Console.SetCursorPosition(0, 0);
                    System.Threading.Thread.Sleep(90);
                }
                if (i % 4 == 0 || i % 7 == 0)
                    System.Threading.Thread.Sleep(200);
                    this.paintPashukAlexOpenPashukCloseAlex();
                Console.SetCursorPosition(5, 64);
                output += str[i];
                Console.WriteLine("\n");
                Console.WriteLine(output);
                Console.SetCursorPosition(0, 0);
                ++i;
            }
        }

        public void AlexPashuk(Alekseev alekseev, Pashuk pashuk)
        {
            alekseev.paintAlekseev();
            Console.SetCursorPosition(83, 83);
            //pashuk.paintPashuk(83, 83);
        }
