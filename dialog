        public void paintPashukAlexAlexOpenBrov()
        {
            int i = 0;
            int j = 0;
            //Console.SetCursorPosition(x, y);
            while (PashukAlexAlexOpenBrov.Length != i)
            {
                // x = y;
                Console.Write(PashukAlexAlexOpenBrov[i]);

                if (i % 119 == 0 && i != 0)
                {
                    Console.WriteLine();
                }
                i++;
                j++;
            }
        }
